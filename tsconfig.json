{
  "extends": "./.wxt/tsconfig.json",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@shadcn": ["./entrypoints"],
      "@shadcn/*": ["./entrypoints/*"],
      "@": ["."],
      "@/*": ["./*"],
      "~": ["."],
      "~/*": ["./*"],
      "@@": ["."],
      "@@/*": ["./*"],
      "~~": ["."],
      "~~/*": ["./*"]
    }
  }
}
